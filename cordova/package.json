{
  "name": "com.appmasknow",
  "displayName": "Masknow",
  "version": "1.1.0",
  "description": "Aplicativo para vendas de mascaras.",
  "main": "index.js",
  "scripts": {
    "clear-folders": "rmdir /q /s node_modules && rmdir /q /s platforms && rmdir /q /s plugins",
    "add-platform-android": "npm run clear-folders && cordova platform add android",
    "build-dev-android": "npm run add-platform-android && cordova build android",
    "build-prod-android": "npm run add-platform-android && cordova build android --prod --release"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Daelton Dias",
  "license": "Apache-2.0",
  "dependencies": {
    "cordova-android": "^8.1.0",
    "cordova-plugin-camera": "^4.1.0",
    "cordova-plugin-geolocation": "^4.0.2",
    "cordova-plugin-keyboard": "^1.2.0",
    "cordova-plugin-network-information": "^2.0.2",
    "cordova-plugin-screen-orientation": "^3.0.2",
    "cordova-plugin-splashscreen": "^5.0.3",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-wkwebview-engine": "^1.2.1",
    "es6-promise-plugin": "^4.2.2"
  },
  "devDependencies": {
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-screen-orientation": {},
      "cordova-plugin-keyboard": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-camera": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+"
      },
      "cordova-plugin-geolocation": {},
      "cordova-plugin-wkwebview-engine": {}
    },
    "platforms": [
      "android"
    ]
  }
}